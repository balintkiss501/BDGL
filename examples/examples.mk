EXAMPLES_BIN_DOS16_DIR = $(EXAMPLES_BIN_DIR)$(SEP)dos16
EXAMPLES_BIN_DOS32_DIR = $(EXAMPLES_BIN_DIR)$(SEP)dos32

$(EXAMPLES_OBJ_DOS16_DIR)/bmp.obj : $(EXAMPLES_SRC_DIR)/bmp.c
	@$(MKDIR_CMD) $(EXAMPLES_OBJ_DOS16_DIR)
	$(C_TO_OBJ16)

$(EXAMPLES_OBJ_DOS32_DIR)/bmp.obj : $(EXAMPLES_SRC_DIR)/bmp.c
