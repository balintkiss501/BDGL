$(EXAMPLES_BIN_DOS16_DIR)/bmp.exe : $(EXAMPLES_OBJ_DOS16_DIR)/bmp.obj $(LIB_DOS16_TARGET)
	@$(MKDIR_CMD) $(EXAMPLES_BIN_DOS16_DIR)
	$(LD) system dos name $@ file $[@ library $(LIB_DOS16_TARGET)
	$(COPY_CMD) $(EXAMPLES_SRC_DIR)$(SEP)IMAGE.BMP $(EXAMPLES_BIN_DOS16_DIR)

$(EXAMPLES_BIN_DOS32_DIR)/bmp.exe : $(EXAMPLES_OBJ_DOS32_DIR)/bmp.obj $(LIB_DOS32_TARGET)
	@$(MKDIR_CMD) $(EXAMPLES_BIN_DOS32_DIR)
	$(LD) system dos4g name $* file $[@ library $(LIB_DOS32_TARGET)
	$(COPY_CMD) $(EXAMPLES_SRC_DIR)$(SEP)IMAGE.BMP $(EXAMPLES_BIN_DOS32_DIR)

